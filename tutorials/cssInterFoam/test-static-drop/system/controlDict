FoamFile
{
version     2.0;
format      ascii;
class       dictionary;
object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
application             interFoam;
startTime               lastTime;
stopAt                  endTime;
endTime                 1e-4;
deltaT                  1e-6;
writeControl            adjustableRunTime;
writeInterval           2e-5;
purgeWrite              0;
writeFormat             ascii;
writePrecision          6;
writeCompression        uncompressed;
timeFormat              scientific;
runTimeModifiable       yes;
adjustTimeStep          on;
maxCo                   0.5;
maxAlphaCo              0.5;
maxDeltaT               1;
