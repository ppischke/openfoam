FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.liq 0
);

regions
(
    boxToCell
    {
        box (-1 -1 -1) (1 1 0);
        fieldValues
        (
            volScalarFieldValue alpha.liq 1
        );
    }
);
// ************************************************************************* //
